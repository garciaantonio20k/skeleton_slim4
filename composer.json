{
    "name": "garciaantonio20k/skeleton_slim4",
    "description": "Slim 4 Skeleton",
    "keywords": ["framework", "slim"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.4.0",
        "slim/slim": "4.10.0",
        "slim/twig-view": "3.3.0",
        "symfony/var-dumper": "^3.2",
        "vlucas/phpdotenv": "^2.4",
        "slim/psr7": "^0.3.0",
        "php-di/php-di": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "scripts": {
        "init-env": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
